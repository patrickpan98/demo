<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>

	<table-filter match-schema="bd_formation" match-name=".*"></table-filter>
	<table-filter match-name="employee" match-catalog="bd_formation"></table-filter>
	<table-filter match-name="department"
		match-catalog="bd_formation">
	</table-filter>
</hibernate-reverse-engineering>